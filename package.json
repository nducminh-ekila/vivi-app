{
  "name": "vivi-app",
  "private": true,
  "lint-staged": {
    "*/src/**/*.{js,jsx,ts,tsx}": [
      "yarn workspace vivi-zma run lint-fix",
      "yarn workspace vivi-zma run prettier-fix",
      "yarn workspace vivi-web-pwa run lint-fix",
      "yarn workspace vivi-web-pwa run prettier-fix"
    ]
  },
  "scripts": {
    "prepare": "husky"
  },
  "workspaces": {
    "packages": [
      "*"
    ],
    "nohoist": [
      "vivi-web/@vite-pwa/assets-generator",
      "vivi-web/@vitejs/plugin-react",
      "vivi-web/vite-plugin-pwa",
      "vivi-web/vite",
      "vivi-web/workbox-build",
      "vivi-web/workbox-core",
      "vivi-web/workbox-window",
      "vivi-zma/@babel/core",
      "vivi-zma/@vitejs/plugin-react",
      "vivi-zma/vite-tsconfig-paths",
      "vivi-zma/vite"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@mui/icons-material": "^6.1.0",
    "@mui/material": "^6.1.0",
    "axios": "^1.7.7",
    "husky": "^9.1.5",
    "lint-staged": "^15.2.7",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.23.0",
    "recoil": "^0.7.7",
    "zmp-ui": "latest"
  },
  "devDependencies": {
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.0",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.6",
    "prettier": "^3.2.5",
    "sass": "^1.76.0",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.2.2"
  }
}
